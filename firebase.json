{
  "functions": {
    "source": "homotopy-server/js"
  },
  "hosting": {
    "target": "homotopy-rs",
    "public": "result",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
